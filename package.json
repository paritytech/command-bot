    "@octokit/webhooks": "9.23.0",
    "async-mutex": "0.3.2",
    "date-fns": "2.28.0",
    "joi": "^17.6.0",
    "level-rocksdb": "5.0.0",
    "lodash": "4.17.21",
    "matrix-bot-sdk": "0.5.19",
    "node-fetch": "^3.2.4",
    "probot": "12.2.2",
    "stoppable": "1.1.0",
    "ts-node": "^10.7.0",
    "yaml": "^2.0.1",
    "yargs": "^17.5.0"
  },
  "devDependencies": {
    "@octokit/auth-app": "3.6.1",
    "@resolritter/tsc-files": "^1.1.4",
    "@types/abstract-leveldown": "7.2.0",
    "@types/levelup": "5.1.1",
    "@types/lodash": "4.14.181",
    "@types/node": "^17.0.24",
    "@types/rocksdb": "3.0.1",
    "@types/shelljs": "0.8.11",
    "@types/stoppable": "1.1.1",
    "@types/yargs": "^17.0.10",
    "@typescript-eslint/eslint-plugin": "5.19.0",
    "@typescript-eslint/parser": "5.19.0",
    "eslint": "8.13.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-prettier": "4.0.0",
    "eslint-plugin-simple-import-sort": "7.0.0",
    "eslint-plugin-sonarjs": "^0.13.0",
    "eslint-plugin-unused-imports": "2.0.0",
    "nodemon": "2.0.15",
    "prettier": "2.6.2",
    "prettier-plugin-compactify": "0.1.6",
    "rimraf": "3.0.2",
    "smee-client": "1.2.2",
    "typescript": "4.6.3"
  },
  "scripts": {
    "typecheck": "tsc --noEmit",
    "prettier": "prettier --loglevel silent '{*,**/*}.{json,html}'",
    "fix:eslint": "eslint --fix",
    "fix:prettier": "prettier --write",
    "fix": "yarn fix:eslint '{*,**/*}.{cjs,ts}' && yarn fix:prettier '{*,**/*}.json'",
    "start:prefix": "node --es-module-specifier-resolution=node --loader ./loader.cjs",
    "start": "yarn start:prefix ./src/main.ts",
    "dev": "yarn start:prefix --trace-warnings -r ./.env.cjs ./src/main.ts",
    "watch": "nodemon --watch src --watch .env.cjs -e cjs,ts --exec \"yarn dev\""
  }
}
